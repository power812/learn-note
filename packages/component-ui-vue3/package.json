{
  "name": "component-ui-vue3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.ts,.vue,.tsx --fix",
    "docs:dev": "npm run register:components && vitepress dev docs",
    "docs:build": "npm run register:components && vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "register:components": "vitepress-rc --componentsDir ../../src/components"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitepress": "1.0.0-rc.10",
    "vitepress-theme-demoblock": "^3.0.3",
    "vue-tsc": "^1.8.5"
  }
}